{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "typeRoots": [
      "./src/types",
      "./node_modules/@types"
    ],
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@/components": [
        "src/components/*"
      ],
      "@/lib": [
        "src/lib/*"
      ],
      "@/layouts": [
        "src/layouts/*"
      ]
    }
  }
}